{
  "roots": ["<rootDir>/src"],
  "moduleFileExtensions": ["js", "jsx", "json", "styl"],
  "moduleNameMapper": {
    "styles": "identity-obj-proxy",
    "\\.svg$": "identity-obj-proxy",
    "drive/(.*)": "<rootDir>/src/drive/$1"
  },
  "transformIgnorePatterns": [
    "node_modules/(?!cozy-ui)/"
  ],
  "testMatch": [
    "**/(*.)(spec|test).js?(x)"
  ]
}
