{
  "extends": "cozy-tsconfig",
  "include": ["./src/**/*"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "emitDeclarationOnly": false,
    "noEmit": true,
    "jsx": "react",
    "paths": {
      "lib/*": ["./src/lib/*"],
      "modules/*": ["./src/modules/*"],
      "components/*": ["./src/components/*"],
      "test/*": ["./test/*"],
      "hooks/*": ["./src/hooks/*"],
      "models/*": ["./src/models/*"],
      "constants/*": ["./src/constants/*"],
      "config/*": ["./src/config/*"],
    }
  }
}
